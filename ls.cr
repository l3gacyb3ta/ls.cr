# grab the files
paths = Dir.new(ARGV.fetch(0, ".")).map do |f|
  Path.new(f) 
end

# sort them
paths.sort! do |a, b|
  (File.directory?(a) == File.directory?(b)) ? 1 : 0
end

# For each path in files
paths.each do |file|

  if File.directory?(file)
    # if it's a directory, make it blue
    puts file.colorize(:blue)
  else

    ext = File.extname(file)
    if ext != "" && extensions.keys.includes? ext
      puts file.colorize extensions[ext]
    elsif ext != ""
      puts file
    else
      puts file.colorize :light_gray
    end
  end
end